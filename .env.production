# Variables d'environnement pour CassKai - PRODUCTION
# ⚠️ ATTENTION: Ne jamais commiter ce fichier avec les vraies valeurs

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# App Configuration
VITE_APP_NAME=CassKai
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# Stripe Configuration (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-live-stripe-key
VITE_API_BASE_URL=https://api.casskai.app

# Analytics & Monitoring (optionnel)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true
VITE_MAINTENANCE_MODE=false