{
  "name": "casskai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CassKai® - Plateforme de gestion financière tout-en-un pour PME et indépendants",
  "author": {
    "name": "NOUTCHE CONSEIL",
    "email": "contact@casskai.app",
    "url": "https://casskai.app"
  },
  "license": "UNLICENSED",
  "homepage": "https://casskai.app",
  "repository": {
    "type": "git",
    "url": "private"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:production",
    "build:fast": "vite build --mode production --minify esbuild",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "build:analyze": "vite build --config vite.config.bundle-analyzer.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:errors": "eslint src --ext ts,tsx --report-unused-disable-directives --quiet",
    "lint:relaxed": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "npx vite preview",
    "type-check": "tsc --noEmit -p tsconfig.app.json",
    "depcheck": "npx depcheck --config depcheck.config.json --skip-missing",
    "validate:db": "node scripts/validate-db-columns.cjs",
    "clean": "rm -rf dist",
    "_prepare": "npm run type-check",
    "deploy": "./scripts/deploy.sh",
    "deploy:quick": "bash scripts/deploy.sh",
    "deploy:fast": "powershell -ExecutionPolicy Bypass -File deploy-fast.ps1",
    "deploy:ultra": "bash deploy-ultra-fast.sh",
    "deploy:skip-build": "bash deploy-ultra-fast.sh --skip-build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ai-categorization": "npx tsx scripts/test-ai-categorization.ts",
    "test:e2e": "playwright test",
    "test:e2e:non-auth": "cross-env PLAYWRIGHT_RUN_AUTHED_E2E=0 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:phase2": "playwright test --config playwright.phase2.config.ts",
    "test:e2e:phase2:ui": "playwright test --config playwright.phase2.config.ts --ui",
    "test:e2e:phase2:headed": "playwright test --config playwright.phase2.config.ts --headed",
    "test:e2e:phase2:report": "playwright test --config playwright.phase2.config.ts && playwright show-report playwright-report-phase2",
    "test:archive": "playwright test e2e/archive-systems.spec.ts",
    "test:archive:ui": "playwright test e2e/archive-systems.spec.ts --ui",
    "test:archive:headed": "playwright test e2e/archive-systems.spec.ts --headed",
    "test:archive:report": "playwright test e2e/archive-systems.spec.ts && playwright show-report",
    "expire-trials": "node scripts/expire-trials.js",
    "trial:stats": "node scripts/expire-trials.js --stats-only",
    "seed:accounting": "node scripts/create-test-accounting-data.mjs",
    "seed:templates": "npx tsx scripts/seed-regulatory-templates.ts",
    "seed:templates:wipe": "npx tsx scripts/seed-regulatory-templates.ts --wipe",
    "generate:seed-sql": "npx tsx scripts/generate-seed-sql.ts",
    "verify:templates": "npx tsx scripts/verify-regulatory-templates.ts",
    "verify:autofill": "npx tsx scripts/verify-autofill-system.ts",
    "kb:ingest:app": "npx tsx scripts/kb/ingest-app-docs.ts"
  },
