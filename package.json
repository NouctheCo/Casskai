{
  "name": "casskai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CassKai - Plateforme de gestion financière tout-en-un pour PME et indépendants",
  "author": "CassKai Team",
  "license": "MIT",
  "homepage": "https://casskai.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/casskai.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "build:safe": "tsc -p tsconfig.production.json --noEmit && npx vite build",
    "build:staging": "npx vite build --mode staging",
    "build:production": "npx vite build --mode production",
    "build:force": "npx vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "npx vite preview",
    "type-check": "npm run type-check:app && npm run type-check:vitest && npm run type-check:playwright",
    "type-check:app": "tsc --noEmit -p tsconfig.app.json",
    "type-check:vitest": "tsc --noEmit -p tsconfig.vitest.json",
    "type-check:playwright": "tsc --noEmit -p tsconfig.playwright.json",
    "clean": "rm -rf dist",
    "_prepare": "npm run type-check && npm run type-check:vitest && npm run type-check:playwright",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "set NODE_ENV=test&& .\\node_modules\\.bin\\vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test tests/e2e/performance.spec.ts",
    "test:accessibility": "playwright test --grep=\"should be accessible\"",
    "test:comprehensive": "npx tsx scripts/run-comprehensive-tests.ts",
    "test:financial": "vitest run src/services/__tests__/invoicingService.test.ts src/services/__tests__/accountingService.test.ts src/services/__tests__/bankReconciliationService.test.ts",
    "test:stripe": "vitest run src/services/__tests__/stripe-integration.test.ts && playwright test tests/e2e/stripe-payment-flow.spec.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "@floating-ui/react": "^0.27.15",
    "@hookform/resolvers": "^3.3.4",
    "@playwright/test": "^1.55.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@stripe/stripe-js": "^7.6.1",
    "@supabase/supabase-js": "^2.45.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/react-grid-layout": "^1.3.5",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-hierarchy": "^3.1.2",
    "d3-sankey": "^0.12.3",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
