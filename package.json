{
  "name": "casskai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CassKai - Plateforme de gestion financière tout-en-un pour PME et indépendants",
  "author": "CassKai Team",
  "license": "MIT",
  "homepage": "https://casskai.app",
