  "scripts": {
    "dev": "vite",
    "build": "npm run build:production",
    "build:fast": "vite build --mode production --minify esbuild",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "npx vite preview",
    "type-check": "tsc --noEmit -p tsconfig.app.json",
    "clean": "rm -rf dist",
    "_prepare": "npm run type-check",
    "deploy": "./scripts/deploy.sh",
    "deploy:quick": "bash scripts/deploy.sh",
    "deploy:fast": "powershell -ExecutionPolicy Bypass -File deploy-fast.ps1",
    "deploy:ultra": "bash deploy-ultra-fast.sh",
    "deploy:skip-build": "bash deploy-ultra-fast.sh --skip-build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "expire-trials": "node scripts/expire-trials.js",
    "trial:stats": "node scripts/expire-trials.js --stats-only",
    "seed:accounting": "node scripts/create-test-accounting-data.mjs"
  },
